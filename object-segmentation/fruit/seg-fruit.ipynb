    "\n",
    "model=YOLO('yolo11n-seg.pt')\n",
    "\n",
    "model=model.to('cuda')\n",
    "datapath='data.yaml'\n",
    "results= model.train(data=datapath, epochs=50, amp=False ,imgsz=640)"
